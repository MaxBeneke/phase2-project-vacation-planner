<h1>Login</h1>

<%= render "/shared/errors" %>

<%= form_tag "handle_login" do %>
    <%= label_tag :username %>
    <%= text_field_tag :username %>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
    <%= submit_tag "Login"%>
<% end %>

<%= link_to "create a new account", new_user_path%>