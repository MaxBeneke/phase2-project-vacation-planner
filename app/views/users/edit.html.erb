<h1>Edit your account information</h1>
<%= render "/shared/errors"%>

<%= form_tag do %>
    <%= label_tag :name %>
    <%= text_field_tag :name %>
    <%= label_tag :username %>
    <%= text_field_tag :username %>
    <%= label_tag :email %>
    <%= email_field_tag :email %>
    <%= submit_tag "Edit Account"%>
<% end %>